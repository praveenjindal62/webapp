{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "mongo",
            "name": "dbapp",
            "hostname": "dbapp"
        },
        {
            "environment": [
                {
                    "name": "MONGOURL",
                    "value": "dbapp:27017"
                },
                {
                    "name": "FLASK_PORT",
                    "value": "5004"
                }
            ],
            "essential": true,
            "image": "praveenjindal62/pyapp",
            "name": "pyapp",
            "portMappings": [
                {
                    "containerPort": 5004,
                    "hostPort": 8080
                }
            ]
        }
    ]
}

