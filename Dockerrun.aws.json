{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "mongo:latest",
            "name": "dbapp",
            "hostname": "dbapp"
        },
        {
            "environment": [
                {
                    "name": "APPURL",
                    "value": "pyapp"
                },
                {
                    "name": "APPPORT",
                    "value": "5004"
                }
            ],
            "essential": true,
            "image": "praveenjindal62/nginx:1.0",
            "name": "nginx",
            "hostname": "nginx",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8080
                }
            ]
        },
        {
            "environment": [
                {
                    "name": "MONGOURL",
                    "value": "dbapp:27017"
                },
                {
                    "name": "FLASK_PORT",
                    "value": "5004"
                }
            ],
            "essential": true,
            "image": "praveenjindal62/pyapp:1.1",
            "name": "pyapp",
            "hostname": "pyapp"
        }
    ],
    "family": "",
    "volumes": []
}

