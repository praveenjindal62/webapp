{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "essential": true,
            "image": "mongo",
            "name": "dbapp",
            "hostname": "dbapp",
            "memory": 128
        },
        {
            "environment": [
                {
                    "name": "MONGOURL",
                    "value": "dbapp:27017"
                },
                {
                    "name": "FLASK_PORT",
                    "value": "5004"
                }
            ],
            "essential": true,
            "image": "praveenjindal62/pyapp:1.1",
            "name": "pyapp",
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 5004,
                    "hostPort": 8080
                }
            ],
            "links": [
                "dbapp"
              ]
        }
    ]
}

